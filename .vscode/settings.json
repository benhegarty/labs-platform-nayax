{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.alwaysShowStatus": true,
  "files.exclude": {
    "": true,
    ".eslintrc.json": true,
    ".prettierrc": true,
    "**/.build": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/*.crswap": true,
    "**/CVS": true,
    "**/node_modules": true,
    "**/Thumbs.db": true
  },
  "editor.tabSize": 2,
  "debug.javascript.automaticallyTunnelRemoteServer": true
}